{
  "canvas": {
    "width": 800,
    "height": 600,
    "containerId": "p5js-container",
    "renderer": "P2D",
    "pixelDensity": 1
  },
  "defaults": {
    "background": {
      "r": 240,
      "g": 240,
      "b": 240
    },
    "fill": {
      "r": 100,
      "g": 100,
      "b": 100
    },
    "stroke": {
      "r": 0,
      "g": 0,
      "b": 0,
      "weight": 1
    }
  },
  "limits": {
    "maxShapes": 50,
    "maxAnimationSpeed": 5,
    "maxSize": 400,
    "minSize": 5
  },
  "animations": {
    "frameRate": 60,
    "smoothing": true,
    "enabledTypes": [
      "rotation",
      "position",
      "size",
      "color"
    ]
  },
  "supportedShapes": [
    "circle",
    "ellipse",
    "rectangle",
    "rect",
    "square",
    "line",
    "triangle",
    "point"
  ],
  "supportedColors": [
    "red",
    "blue",
    "green",
    "yellow",
    "orange",
    "purple",
    "pink",
    "black",
    "white",
    "gray",
    "brown"
  ]
}
